$HOME=G:\TESTmon\TESTamm_exporter
$INSTANCE=MONIPRD
$PORT=9080
$JAVA_HOME=&HOME\java\jdk-11.0.2
$YAML=&HOME\configs
$JAR=&HOME\TEST-application-monitoring.jar

$SERVICE.NAME=TEST_AMM(&INSTANCE)
$SERVICE.DISPLAY_NAME=TEST AMM EXPORTER(&INSTANCE)
$NLS_LANG=AMERICAN_AMERICA.UTF8
$SERVICE.CMD_LINE=&JAVA_HOME\bin\java.exe -cp &HOME -Xmx96M -Dserver.port=&PORT -Duser.language=en -Duser.country=US -DloopingInterval=3 -DinactivitySleep=7200 -DconfFolder=&YAML -jar &JAR
$SERVICE.CHECK_INTERVAL=300000000
$SERVICE.RESTART_THRESHOLD=300000000
$SERVICE.RESTART_DELAY=5
$SERVICE.LOG=Y
$SERVICE.LOG_FILE=&HOME\logs\TEST_AMM_service.log
$SERVICE.PRIORITY_CLASS=NORMAL
$SERVICE.SHOW_GUI=Y
$SERVICE.AUTO_START=Y
$SERVICE.ENV.NLS_LANG=&NLS_LANG
